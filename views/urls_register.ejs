<!DOCTYPE html>
<html lang="en">
  <head>
    <title>TinyApp</title>
  </head>
  <body>
      <!-- Add your EJS code here -->
      <header><% include ./partials/_header %></header>
      <form action="/register" method="POST" style="margin:150px;">
        <label for="register">E-mail:</label>
        <input id="email" type="email" name="email" placeholder="example@email.com" style="width: 300px">
        <label for="password">Password:</label>
        <input id="password" type="password" name="password" placeholder="Password" style="width: 300px">
        <input type="submit" value="Submit">
      </form>
  </body>
</html>